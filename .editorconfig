# https://editorconfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{bat,cmd,ps1}]
end_of_line = crlf

[*.sh]
end_of_line = lf

[pom.xml]
indent_style = tab

[.vscode/**.json]
insert_final_newline = false

[.idea/**]
insert_final_newline = false

[**.java]
indent_style = tab
